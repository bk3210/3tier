<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Dept">
	<select id="Dept_getList" parameterType="DeptDTO" resultType="DeptDTO">
		SELECT
			dept_no
			, dept_id
			, dept_nm
			, dept_oper_yn
		FROM dept
		ORDER BY dept_id		
	</select>
	
